[sub_resource type="GDScript" id="GDScript_5rs3a"]
resource_name = "SettingsController"
script/source = "extends MarginContainer

@onready var map_size_x : LineEdit = $\"Settings Area Main/Settings Section/Section Margin/Settings Main/Map Settings/Size X/Size X Input\"
@onready var map_size_y : LineEdit = $\"Settings Area Main/Settings Section/Section Margin/Settings Main/Map Settings/Size Y/Size Y Input\"
@onready var corners : LineEdit = $\"Settings Area Main/Settings Section/Section Margin/Settings Main/Map Settings/Corners/Corners Input\"
@onready var centralizedSpawn : CheckBox = $\"Settings Area Main/Settings Section/Section Margin/Settings Main/Boid Settings/CentralizedSpawn/CentralizedSpawn Input\"
@onready var count : LineEdit = $\"Settings Area Main/Settings Section/Section Margin/Settings Main/Boid Settings/Count/Count Input\"

# Restart simulation
func _on_restart_pressed() -> void:
	# Read & Save data which requires restart button
	Settings.map_size.x = float(map_size_x.text)
	Settings.map_size.y = float(map_size_y.text)
	Settings.corners = float(corners.text)
	Settings.centralizedSpawn = centralizedSpawn.button_pressed
	Settings.count = int(count.text)
	
	$\"../../BoidController\"._ready()

# Change speed of boids
func _on_speed_input_text_changed(new_text: String) -> void:
	Settings.speed = float(new_text)

# Change trail weight of boids
func _on_deposit_rate_input_text_changed(new_text: String) -> void:
	Settings.depositRate = float(new_text)

# Change angle of sensors
func _on_angle_input_text_changed(new_text: String) -> void:
	var angle_deg : float = float(new_text)
	Settings.sensor_angle = deg_to_rad(angle_deg)

# Change distance of sensors
func _on_distance_input_text_changed(new_text: String) -> void:
	Settings.sensor_distance = float(new_text)

# Change size of sensors
func _on_size_input_text_changed(new_text: String) -> void:
	Settings.sensor_size = float(new_text)

# Change threshold of sensors
func _on_threshold_input_text_changed(new_text: String) -> void:
	Settings.sensor_threshold = float(new_text)

# Change blue size of decay
func _on_blur_size_input_text_changed(new_text: String) -> void:
	Settings.blurSize = float(new_text)

# Change smoothing speed of decayed value
func _on_smoothing_input_text_changed(new_text: String) -> void:
	Settings.smoothing = float(new_text)

# Change decay rate of trailmap
func _on_decay_rate_input_text_changed(new_text: String) -> void:
	Settings.decayRate = float(new_text)
"

