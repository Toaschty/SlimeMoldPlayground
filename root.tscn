[sub_resource type="GDScript" id="GDScript_7m0rh"]
script/source = "extends PanelContainer

@onready var render_viewport : SubViewport = $RenderViewport

# Handle saving of images
func _on_save_button_pressed() -> void:
	var time_dict := Time.get_datetime_dict_from_system()
	var filename : String = \"./export_%d%d%d.png\" % [time_dict.hour, time_dict.minute, time_dict.second]
	var increment = 0
	
	# Check for already existing files
	while FileAccess.file_exists(filename):
		increment += 1
		filename = \"./Exports/export_%d%d%d_%d.png\" % [time_dict.hour, time_dict.minute, time_dict.second, increment]
	
	render_viewport.get_texture().get_image().save_png(filename)

# Update viewport resolution
func _on_simulate_pressed() -> void:
	render_viewport.size = Settings.map_size
"

